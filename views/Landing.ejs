  <!-- intro banner -->
  <div class="main-banner">
    <div class="banner-content container">
      <div class="banner-text">
        <img src="https://ocmain.s3-us-west-1.amazonaws.com/Groupie+Resources/GoogleDrive+Groupie/LogoTransparent_2.png" height="15%" alt="Groupie">
        <h4 class="text-white my-3">A new way to interact with your favorite artists</h4>
        <button class="groupie-button" type="button" ><b>Learn More!</b></button>
      </div>
      <div class="banner-right">
      </div>  
    </div>
